.page-header
  %h1
    Create New Account
%ul.breadcrumb
  %li
    %a{:href => "/dashboard"} Dashboard
    %span{:class => "divider"} /
  %li
    %span New Account
=form_for @account, :class => "form-stacked" do |f|
  .form-stacked
    %fieldset
      =f.label :account_name
      =f.text_field :name, :class => "xlarge error"
      %br
      %br
      =f.submit :class => "btn primary"

